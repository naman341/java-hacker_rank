import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner obj =new Scanner(System.in);
        int a =obj.nextInt();
        int b=a;
        for(int i=1;i<=a;i++)
        {
            for(int j=1;j<=b;j++)
            {
               System.out.print(j+" ");
            }
            System.out.println("");
            b=b-1;
        }
        
    }
}
